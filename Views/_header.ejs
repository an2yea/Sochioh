<header>
  <link rel="stylesheet" type="text/css" href="/css/header" />
  <div id="Logo">
    <a href="/">
      <img src="/images/logo1.jpg" />
    </a>
  </div>
  <div id="nav">
    <ul>
      <% if(locals.user) { %>
      <li>
        <a href="/users/profile/<%= user.id %>">
          <img
            src="<%= user.avatar%>"
            alt="<%= user.name %>"
            height="60px"
            width="60px"
          /><%= user.name %></a
        >
      </li>
      <li>
        <a href="/users/sign_out"> Sign Out </a>
      </li>
      <% } else { %>
      <li>
        <a href="/users/sign_in"> Sign In </a>
      </li>
      <li>
        <a href="/users/sign_up"> Sign Up</a>
      </li>
      <% } %>
    </ul>
  </div>
</header>
